{"name":"Tavern","flags":{"monks-enhanced-journal":{"type":"shop","purchasing":"confirm","selling":"confirm","items":[{"_id":"XBAMdJ48AXQYjONo","name":"Wheat","type":"loot","img":"icons/consumables/grains/sack-oats-glowing-white.webp","data":{"description":{"value":"<p>A common ingredient for bread, beer, and more; this staple often forms the base of a diet for people where their land is open and relatively flat, the weather temperate, and the area safe enough for farmers to operate.</p>","chat":"","unidentified":""},"source":"PHB pg. 157","quantity":1,"weight":1,"price":"1 cp","attunement":0,"equipped":false,"rarity":"Common","identified":true,"attuned":false,"damage":{"parts":[]},"cost":"1 cp"},"effects":[],"folder":null,"sort":1700000,"permission":{"default":0},"flags":{},"hide":false,"lock":false},{"_id":"EBmuvyXO3Edt6Tf3","name":"Flour","type":"loot","img":"icons/consumables/food/soup-broth-bowl-wooden-yellow.webp","data":{"description":{"value":"<p>Grain that has been ground by a miller in the next step along the food chain.</p>","chat":"","unidentified":""},"source":"PHB pg. 157","quantity":1,"weight":1,"price":"2 cp","attunement":0,"equipped":false,"rarity":"Common","identified":true,"attuned":false,"damage":{"parts":[]},"cost":"2 cp"},"effects":[],"folder":null,"sort":1800000,"permission":{"default":0},"flags":{},"hide":false,"lock":false},{"_id":"W6jUmqvN7lAdPNeG","name":"Salt","type":"loot","img":"icons/consumables/food/bowl-stew-brown.webp","data":{"description":{"value":"<p>Prized by chefs and quartermasters alike as a seasoning and preservative for supplies where alcohol or vinegar are undesired.</p>","chat":"","unidentified":""},"source":"PHB pg. 157","quantity":1,"weight":1,"price":"5 cp","attunement":0,"equipped":false,"rarity":"Common","identified":true,"attuned":false,"damage":{"parts":[]},"cost":"5 cp"},"effects":[],"folder":null,"sort":2000000,"permission":{"default":0},"flags":{},"hide":false,"lock":false},{"_id":"TRxBiy2ZILndIGMd","name":"Cinn